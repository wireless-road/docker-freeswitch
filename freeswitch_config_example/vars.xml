<include>
  <X-PRE-PROCESS cmd="set" data="global_codec_prefs=PCMU,PCMA,G722"/>

  <!-- NOTE: CHANGE THIS BEFORE USE -->
  <X-PRE-PROCESS cmd="set" data="xml_rpc_password=worksnot"/>
	
	
	<X-PRE-PROCESS cmd="set" data="internal_auth_calls=true"/>
	<X-PRE-PROCESS cmd="set" data="internal_sip_port=5060"/>
	<X-PRE-PROCESS cmd="set" data="internal_tls_port=5061"/>
	<X-PRE-PROCESS cmd="set" data="internal_ssl_enable=false"/>

	<X-PRE-PROCESS cmd="set" data="external_rtp_ip=stun:stun.freeswitch.org"/>
	<X-PRE-PROCESS cmd="set" data="external_sip_ip=stun:stun.freeswitch.org"/>

	<X-PRE-PROCESS cmd="set" data="external_auth_calls=false"/>
	<X-PRE-PROCESS cmd="set" data="external_sip_port=5080"/>
	<X-PRE-PROCESS cmd="set" data="external_tls_port=5081"/>
	<X-PRE-PROCESS cmd="set" data="external_ssl_enable=false"/>

	<X-PRE-PROCESS cmd="set" data="send_silence_when_idle=600"/>
	<X-PRE-PROCESS cmd="set" data="sound_prefix=$${sounds_dir}/ru/RU/elena"/>
